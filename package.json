{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node build.cjs",
    "build:dev": "vite build --mode development",
    "build:backend": "tsc -p tsconfig.backend.json && babel dist/{backend,shared} -d dist --extensions .js,.ts",
    "build:frontend": "vite build && tsc -p tsconfig.frontend-server.json && babel dist/frontend -d dist/frontend --extensions .js,.ts",
    "pkg:full": "npm run build:frontend && npm run build:backend && pkg dist/backend/server.js --target node18-win-x64 --output EasyDeploy",
    "server": "tsx backend/server.ts",
    "start": "npm run server",
    "start:prod": "node EasyDeploy.exe"
  },
  "dependencies": {

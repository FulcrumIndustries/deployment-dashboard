    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "bin": "EasyDeploy.exe",
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "snapshot": {
      "dir": "snapshot-cache",
      "bypass": "**/*.ts"
    },
    "assets": [
      "dist/assets/**/*",
      "dist/index.html"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "executables",
    "externals": [
      "ws",
      "express",
      "dexie",
      "@vitejs/plugin-react-swc",
      "vite",
      "default-browser",
      "default-browser-id",
      "is-docker",
      "is-wsl",
      "internmap",
      "run-applescript",
      "string-width",
      "strip-ansi",
      "wrap-ansi",
      "ansi-regex",
      "ansi-styles"
    ]
  }
